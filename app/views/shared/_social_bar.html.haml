.social_bar
  .container
    .row-fluid
      .span10
        %h2= @page_sub_title
      .span2.hidden-phone.hidden-tablet
        %ul
          %li= link_to image_tag("social_bar/twitter.png"), "http://twitter.com/betacube", :target => "betacube"
          %li= link_to image_tag("social_bar/facebook.png"), "https://www.facebook.com/betacube", :target => "betacube"
          %li= link_to image_tag("social_bar/youtube.png"), "http://youtube.com/user/betacubeeu", :target => "betacube"
          %li= link_to image_tag("social_bar/github.png"), "https://github.com/betacube/betacube.eu", :target => "betacube"
